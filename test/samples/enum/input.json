{
  "id": "http://yourdomain.com/schemas/myschema.json",
  "description": "Enum example",
  "type": "object",
  "properties": {
    "gender": {
      "description": "the gender",
      "type": "string",
      "enum": ["Male", "Female"]
    },
    "color": {
      "description": "the color",
      "type": ["null", "string"],
      "enum": [null, "blue", "red"]
    }
  },
  "required": ["gender"]
}
